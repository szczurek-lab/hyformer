{
  "compile": true,
  "enable_ddp": true,
  "dtype": "float32",
  "eval_only": false,
  "max_iters": 600,
  "batch_size": 2,
  "learning_rate": 6e-4,
  "weight_decay": 1e-1,
  "beta1": 0.9,
  "beta2": 0.95,
  "gradient_accumulation_steps": 2,
  "grad_clip": 1.0,
  "decay_lr": true,
  "warmup_iters": 60,
  "lr_decay_iters": 600,
  "min_lr": 1e-6,
  "eval_iters": 2,
  "eval_interval": 200,
  "log_interval": 100,
  "always_save_checkpoint": false,
  "save_checkpoint_every": 200,
  "block_size": 128,
  "tasks":
    {
      "lm": 1,
      "physchem":  1
    }
}